{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "eslint:recommended"
  ],
  "env": {
    "browser": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "@typescript-eslint/array-type": [
      "error",
      { "default": "generic" }
    ],
    "@typescript-eslint/no-var-requires": 0,
    "no-var": 1
  }
}
